{
  "name": "azurefunctionsexamplebackend",
  "version": "1.0.0",
  "description": "Azure Functions Example Backend with TypeScript and v4 Programming Model",
  "main": "dist/functions.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prestart": "npm run build",
    "start": "func start",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "predeploy": "npm run clean && npm run build && npm run lint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "azure",
    "functions",
    "serverless",
    "typescript",
    "backend",
    "api"
  ],
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@azure/functions": "^4.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  }
}
